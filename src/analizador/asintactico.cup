COND ::= BOOLEANVAL: i{:RESULT = i;:} 
| NOT COND :i{:
    if(i.equals("TRUE")){
        RESULT = "FALSE";
    }else if(i.equals("FALSE")){
        RESULT = "TRUE";
    }
:} 
| EXPNUM:e1 OP_REL:o EXPNUM:e2 {:
    if(o.equals("EQUAL_OP")){
        RESULT = Integer.parseInt(e1.toString()) == Integer.parseInt(e2.toString());
    }else if(o.equals("GREATER_OP")){
        RESULT = Integer.parseInt(e1.toString()) > Integer.parseInt(e2.toString());
